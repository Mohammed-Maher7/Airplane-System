@model AirplaneMVC.Models.Employee_Qualifications
@using AirplaneMVC.Models

@{
    ViewData["Title"] = "Delete Qualification";
}

<h2>Delete Qualification</h2>

<hr />

<h4>Are you sure you want to delete this qualification?</h4>

<dl class="row">
    <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Skill)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.Skill)</dd>

    <dt class="col-sm-2">Employee</dt>
    <dd class="col-sm-10">@Model.Employee?.Name</dd>
</dl>

<form asp-action="Delete">
    @Html.AntiForgeryToken()
    <input type="hidden" asp-for="QualificationID" />
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>